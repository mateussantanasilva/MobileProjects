{
  "cli": {
    "version": ">= 9.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "autoIncrement": true,
      "ios": {
        "simulator": true,
        "autoIncrement": "buildNumber"
      },
      "android": {
        "distribution": "internal",
        "autoIncrement": "versionCode"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "autoIncrement": "versionCode"
      },
      "ios": {
        "image": "latest",
        "autoIncrement": "buildNumber"
      },
      "channel": "production"
    }
  }
